     mkdir api_v1
     cd api_v1/
     npm install --save sequelize
     npm init -y
     sudo apt-get install mariadb
     sudo apt-get install mariadb-server
     npm install --save mariadb
     sudo npm install express-generator -g
     npm install 
     npm install sequelize --save
     npm install sequelize-cli --save
     npx sequelize init
     DEBUG=api:* npm start



     npx sequelize-cli model:generate --name carreras --attributes nombre:string,id_instituto:integer
     npx sequelize-cli model:generate --name materias --attributes nombre:string,id_carrera:integer
     npx sequelize-cli model:generate --name alumnos --attributes nombre:string,apellido:string,id_carrera:integer
     npx sequelize-cli model:generate --name institutos --attributes nombre:string
     npx sequelize db:migrate

